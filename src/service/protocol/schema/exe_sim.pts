
msg ExeSimReadLockAcquire
{
    request(mega::network::ConversationID simulationID);
    response(mega::TimeStamp timestamp);
}
msg ExeSimWriteLockAcquire
{
    request(mega::network::ConversationID simulationID);
    response(mega::TimeStamp timestamp);
}
msg ExeSimLockRelease
{
    request(mega::network::ConversationID simulationID);
    response();
}

