<script type="text/javascript">

    currentReportType = "home"

    const queryString = window.location.search
    const urlParams = new URLSearchParams(queryString);
    
    if( urlParams.has( "report" ) )
    {
        currentReportType = urlParams.get( "report" )
        console.log( "Report Type is: " + currentReportType )
    }

    /*
    a 97
    b 98
    c 99
    d 100
    e 101
    f 102
    g 103
    h 104
    i 105
    j 106
    k 107
    l 108
    m 109
    n 110
    o 111
    p 112
    q 113
    r 114
    s 115
    t 116
    u 117
    v 118
    w 119
    x 120
    y 121
    z 122
    */

    document.onkeypress = function (e) 
    {
        e = e || window.event;
        lastKeyPressed = e.keyCode
        console.log( "onkeypress= " + lastKeyPressed )

        if( lastKeyPressed == 105 ) // i
        {
            currentReportType = "interface"
        }
        else if( lastKeyPressed == 99 ) // c
        {
            currentReportType = "concrete"
        }
        else if( lastKeyPressed == 115 ) // s
        {
            currentReportType = "symbols"
        }
        else if( lastKeyPressed == 109 ) // m
        {
            currentReportType = "memory"
        }
        else if( lastKeyPressed == 104 ) // h
        {
            currentReportType = "home"
        }
        console.log( "Report Type is: " + currentReportType )
    };

    // your handler
    function navigateTo( path, params, fragment ) 
    {
        if( params )
        {
            newLocation = path + "?" + params + "&report=" + currentReportType
        }
        else
        {
            newLocation = path + "?report=" + currentReportType
        }

        if( fragment )
        {
            newLocation = newLocation + "#" + fragment
        }

        console.log( "href= " + newLocation + "\n" )

        window.location.href = newLocation
    }

</script>

{{ body }}
</body>

